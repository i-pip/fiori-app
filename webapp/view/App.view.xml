<mvc:View
		height="100%"
		displayBlock="true"
		xmlns="sap.f"
		xmlns:mvc="sap.ui.core.mvc">

	<FlexibleColumnLayout
			id="flexibleColumnLayout"
			backgroundDesign="Solid">
		<beginColumnPages>
			<mvc:XMLView
					id="beginView"
					viewName="pip.fiori-app.view.Master" />
		</beginColumnPages>
		<midColumnPages>
			<mvc:XMLView
					id="detailView"
					viewName="pip.fiori-app.view.Detail" />
		</midColumnPages>
	</FlexibleColumnLayout>
</mvc:View>